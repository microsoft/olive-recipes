{
    "input_model": {
        "type": "PyTorchModel",
        "model_path": "openai/whisper-large-v3-turbo",
        "model_loader": "get_encoder_decoder_init",
        "model_script": "user_script.py",
        "io_config": "get_encoder_decoder_init_io_config",
        "dummy_inputs_func": "get_encoder_decoder_init_dummy_inputs"
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "gpu", "execution_providers": [ "CUDAExecutionProvider" ] } ]
        }
    },
    "passes": {
        "convert": { "type": "OnnxConversion" },
        "to_fp16": { "type": "OnnxFloatToFloat16" },
        "optimize": {
            "type": "OrtTransformersOptimization",
            "model_type": "bart",
            "num_heads": 0,
            "hidden_size": 0,
            "opt_level": 0,
            "optimization_options": {
                "use_multi_head_attention": true
            },
            "only_onnxruntime": false
        }
    },
    "host": "local_system",
    "target": "local_system",
    "no_artifacts": true,
    "output_dir": "models/encoder_decoder_init"
}
