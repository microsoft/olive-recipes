{
    "input_model": {
        "type": "HfModel",
        "model_path": "openai/whisper-large-v3-turbo"
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [
                {
                    "device":"npu",
                    "execution_providers": [
                        "OpenVINOExecutionProvider"
                    ]
                }
            ]
        }
    },
    "passes": {
        "optimum_convert": {
            "type": "OpenVINOOptimumConversion",
            "extra_args": {
                "device": "npu"
            },
            "ov_quant_config": {
                "weight_format": "fp16",
                "trust_remote_code": true
            }
        },
        "io_update": {
            "type": "OpenVINOIoUpdate",
            "static": false,
            "reuse_cache": true
        }
    },
    "target": "local_system",
    "cache_dir": "cache",
    "evaluate_input_model": false,
    "output_dir": "model/whisper-large-v3-turbo"
}
