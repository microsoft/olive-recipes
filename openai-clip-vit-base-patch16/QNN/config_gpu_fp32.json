{
    "input_model": {
        "type": "HfModel",
        "model_path": "openai/clip-vit-base-patch16",
        "task": "zero-shot-image-classification",
        "load_kwargs": {
            "attn_implementation": "eager"
        },
        "io_config": {
            "input_names": [
                "input_ids",
                "pixel_values",
                "attention_mask"
            ],
            "input_shapes": [
                [
                    10,
                    77
                ],
                [
                    1,
                    3,
                    224,
                    224
                ],
                [
                    10,
                    77
                ]
            ],
            "input_types": [
                "int64",
                "float32",
                "int64"
            ],
            "output_names": [
                "logits_per_image",
                "logits_per_text",
                "text_embeds",
                "image_embeds"
            ],
            "output_shapes": [
                [
                    1,
                    10
                ],
                [
                    10,
                    1
                ],
                [
                    10,
                    512
                ],
                [
                    1,
                    512
                ]
            ]
        }
    },
    "systems": {
        "target_system": {
            "type": "LocalSystem",
            "accelerators": [
                {
                    "device": "gpu",
                    "execution_providers": [
                        "QNNExecutionProvider"
                    ]
                }
            ]
        }
    },
    "passes": {
        "conversion": {
            "type": "OnnxConversion",
            "target_opset": 20,
            "save_as_external_data": false
        },
        "surgery": {
            "type": "GraphSurgeries",
            "surgeries": [
                {
                    "surgeon": "MatMulAddToGemm"
                },
                {
                    "surgeon": "ReplaceAttentionMaskValue",
                    "replacement": -50.0
                }
            ]
        }
    },
    "target": "target_system",
    "cache_dir": "cache",
    "output_dir": "output",
    "evaluate_input_model": false,
    "clean_cache": true
}
