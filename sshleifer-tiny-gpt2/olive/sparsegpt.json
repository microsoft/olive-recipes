{
    "input_model": {
        "type": "HFModel",
        "model_path": "sshleifer/tiny-gpt2",
        "task": "text-generation"
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "cpu", "execution_providers": [ "CPUExecutionProvider" ] } ]
        }
    },
    "data_configs": [
        {
            "name": "train_data",
            "type": "HuggingfaceContainer",
            "load_dataset_config": {
                "data_name": "wikitext",
                "subset": "wikitext-2-raw-v1",
                "split": "train[:1000]"
            },
            "pre_process_data_config": {
                "strategy": "join-random",
                "max_seq_len": 1024,
                "max_samples": 1,
                "pad_to_max_len": true,
                "random_seed": 42
            }
        }
    ],
    "passes": {
        "s": {
            "type": "SparseGPT",
            "device": "cpu",
            "sparsity": [2, 4],
            "data_config": "train_data"
        }
    },
    "host": "local_system",
    "target": "local_system",
    "output_dir": "models"
}
