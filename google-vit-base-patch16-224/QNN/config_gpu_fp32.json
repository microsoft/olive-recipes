{
    "input_model": {
        "type": "HfModel",
        "model_path": "google/vit-base-patch16-224",
        "task": "image-classification",
        "io_config": {
            "input_names": [
                "pixel_values"
            ],
            "input_shapes": [
                [
                    1,
                    3,
                    224,
                    224
                ]
            ],
            "output_names": [
                "logits"
            ]
        }
    },
    "systems": {
        "target_system": {
            "type": "LocalSystem",
            "accelerators": [
                {
                    "device": "gpu",
                    "execution_providers": [
                        "QNNExecutionProvider"
                    ]
                }
            ]
        }
    },
    "passes": {
        "conversion": {
            "type": "OnnxConversion",
            "target_opset": 20,
            "save_as_external_data": false
        },
        "surgery": {
            "type": "GraphSurgeries",
            "surgeries": [
                {
                    "surgeon": "MatMulAddToGemm"
                }
            ]
        }
    },
    "target": "target_system",
    "cache_dir": "cache",
    "output_dir": "output",
    "evaluate_input_model": false,
    "clean_cache": true
}
