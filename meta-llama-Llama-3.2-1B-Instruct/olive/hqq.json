{
    "input_model": {
        "type": "HfModel",
        "model_path": "meta-llama/Llama-3.2-1B-Instruct"
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "gpu", "execution_providers": [ "CUDAExecutionProvider" ] } ]
        }
    },
    "passes": {
        "m": { "type": "ModelBuilder", "precision": "fp16" },
        "q": { "type": "OnnxHqqQuantization" }
    },
    "host": "local_system",
    "target": "local_system",
    "output_dir": "models"
}
